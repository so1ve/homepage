{
  "private": true,
  "packageManager": "pnpm@7.19.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate && esbuild --minify .output/public/sw.js --outfile=.output/public/sw.js --allow-overwrite",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.1.4",
    "@kevinmarrec/nuxt-pwa": "^0.15.0",
    "@nuxtjs/google-fonts": "3.0.0",
    "@so1ve/eslint-config": "^0.46.0",
    "@unocss/nuxt": "^0.49.0",
    "@unocss/reset": "^0.49.0",
    "eslint": "^8.32.0",
    "nuxt": "3.1.1",
    "typescript": "^4.9.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "webpack"
      ]
    }
  }
}
