{
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate && esbuild --minify .output/public/sw.js --outfile=.output/public/sw.js --allow-overwrite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.1.4",
    "@kevinmarrec/nuxt-pwa": "^0.17.0",
    "@nuxtjs/google-fonts": "3.0.1",
    "@so1ve/eslint-config": "^0.97.0",
    "@unocss/nuxt": "^0.52.0",
    "@unocss/reset": "^0.52.0",
    "eslint": "^8.32.0",
    "nuxt": "3.5.1",
    "typescript": "^5.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "webpack"
      ]
    }
  }
}
